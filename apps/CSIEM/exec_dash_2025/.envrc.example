# Example environment variables for Sumo Logic Terraform provider
# Copy this to .envrc (if using direnv) or source directly

# Sumo Logic Access ID
export SUMO_ACCESS_ID="your-access-id"
export TF_VAR_access_id="$SUMO_ACCESS_ID"

# Sumo Logic Access Key
export SUMO_ACCESS_KEY="your-access-key"
export TF_VAR_access_key="$SUMO_ACCESS_KEY"

# Sumo Logic Endpoint/Environment (e.g., au, us2, in, etc.)
export SUMO_ENDPOINT="au"
export TF_VAR_environment="$SUMO_ENDPOINT"
