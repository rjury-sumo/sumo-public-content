            "visualSettings": "{\"general\":{\"mode\":\"TextPanel\",\"type\":\"text\",\"displayType\":\"default\"},\"title\":{\"fontSize\":14},\"series\":{},\"text\":{\"format\":\"markdown\",\"backgroundColor\":\"#fcfbfb\",\"textColor\":\"black\",\"fontSize\":14}}",
